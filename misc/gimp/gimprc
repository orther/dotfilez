# GIMP gimprc
#
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(layer-preview-size small)
(monitor-xresolution 101.000000)
(monitor-yresolution 101.000000)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (padding-mode default)
    (padding-color (color-rgb 1.000000 1.000000 1.000000)))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (padding-mode default)
    (padding-color (color-rgb 1.000000 1.000000 1.000000)))
(theme "Gimp-CC-Theme-Darker")

(default-snap-to-canvas yes)
(default-snap-to-grid yes)

# end of gimprc
